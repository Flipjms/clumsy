{
  "name": "ClumsyCMS",
  "description": "A CMS for Laravel",
  "version": "0.28.0",
  "devDependencies": {
    "bootstrap-sass": "^3.3.7",
    "gulp": "^3.9.1",
    "jquery": "^3.1.0",
    "laravel-elixir": "^6.0.0-9",
    "laravel-elixir-browserify-official": "^0.1.3",
    "laravel-elixir-eslint": "^3.0.0"
  },
  "eslintConfig": {
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        },
        "env": {
          "es6": true,
          "browser": true
        }
    },
    "globals": {
        "$": false,
        "handover": false
    }
  }
}
