{
    "name": "clumsy/cms",
    "description": "A CMS for Laravel",
    "keywords": ["laravel", "cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Buteler",
            "email": "tbuteler@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "5.1.*",
        "laravelcollective/html": "5.1.*",
        "clumsy/assets": "0.7.*",
        "clumsy/utils": "0.9.*",
        "clumsy/eminem": "0.9.*"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Clumsy\\CMS\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.23.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
