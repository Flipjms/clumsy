{
    "name": "clumsy/cms",
    "description": "A CMS for Laravel",
    "keywords": ["laravel", "cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Buteler",
            "email": "tbuteler@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "5.3.*",
        "laravelcollective/html": "5.3.*",
        "clumsy/assets": ">=0.7.0",
        "clumsy/utils": ">=0.10.0",
        "clumsy/eminem": ">=0.13.0"
    },
    "autoload": {
        "psr-4": {
            "Clumsy\\CMS\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.28.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
