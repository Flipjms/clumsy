{
    "name": "clumsy/cms",
    "description": "A CMS for Laravel",
    "keywords": ["laravel", "cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Buteler",
            "email": "tbuteler@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "cartalyst/sentry": "2.1.*",
        "clumsy/locale": "dev-master",
        "clumsy/assets": "dev-master",
        "clumsy/utils": "dev-master",
        "clumsy/eminem": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Clumsy\\CMS": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8.13-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
